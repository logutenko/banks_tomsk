        android:layout_marginLeft="5dp">
    </TextView>
    <TextView
        android:id="@+id/tvSellCurrency2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp">
    </TextView>
